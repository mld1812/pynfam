    use_fam_storage = 0
    real_eqrpa = 2.0
    imag_eqrpa = 0.5
/

&ext_field
    beta_type = '-'
    operator_name = 'P'
    operator_k = 0
    compute_crossterms = .true.
    two_body_current_mode = 111020
