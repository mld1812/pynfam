    override_csr = 0.0
    override_cds = 0.0
    override_ct = ,
    override_cgs = ,
    override_cf = ,
/

&solver
    max_iter = 5
    convergence_epsilon = 1e-07
    broyden_history_size = 50
    energy_shift_prot = 0.0
    energy_shift_neut = 0.0
    quench_residual_int = 1.0
/
